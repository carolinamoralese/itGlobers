{
  "header-row#main-menu-desktop": {
    "title": "header main menu",
    "children": ["flex-layout.row#desktop-section-one-main-information"],
    "props": {
      "blockClass": "container-menu-desktop"
    }
  },

  "flex-layout.row#desktop-section-one-main-information": {
    "title": "menu showing main information",
    "props": {
      "horizontalAlign": "center",
      "verticalAlign": "center",
      "preventHorizontalStretch": true,
      "preventVerticalStretch": true,
      "fullWidth": true,
      "blockClass": "header-container-menu-desktop-section-one"
    },
    "children": [
      "flex-layout.row#section-left-menu-logo-search",
      "flex-layout.row#section-right-information-carButton-login"
    ]
  },
  "flex-layout.row#section-left-menu-logo-search": {
    "title": "section menu, logo y search",
    "props": {
      "horizontalAlign": "center",
      "verticalAlign": "center",
      "preventHorizontalStretch": true,
      "preventVerticalStretch": true,
      "fullWidth": true,
      "blockClass": "header-container-menu-desktop-section-one-left"
    },
    "children": [
     "flex-layout.col#meda-menu-desktop", 
      "flex-layout.col#logo-desktop", 
      "search-bar"]
  },
  "flex-layout.row#section-right-information-carButton-login": {
    "title": "section information, cart button y login",
    "props": {
      "horizontalAlign": "center",
      "verticalAlign": "center",
      "preventHorizontalStretch": true,
      "preventVerticalStretch": true,
      "fullWidth": true,
      "blockClass": "header-container-menu-desktop-section-one-right"
    },
    "children": [
      "menu-item#trends",
      "menu-item#colecionts",
      "menu-item#offerts",
      "menu-item#cyber-day",
      "menu-item#never-say-never",
      "minicart.v2",
      "login#header-desktop"
    ]
  },
